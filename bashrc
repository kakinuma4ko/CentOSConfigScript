export PATH="$PATH:$HOME/.rvm/bin" # Add RVM to PATH for scripting

export LD_LIBRARY_PATH="/user/local/lib" #for crfpp

alias mdstp='cd ~/desktop'
alias mdevp='cd ~/developer'
alias mbsrc='emacs ~/.bashrc'
alias mfinderhide='defaults write com.apple.finder AppleShowAllFiles -bool false'
alias mfindershow='defaults write com.apple.finder AppleShowAllFiles -bool true'
alias gitacp='git add .;git commit -m "commit by alias";git push;'
